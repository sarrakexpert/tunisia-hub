#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/projects/ke/code/test-maker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/projects/ke/code/test-maker/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sparkline@0.1.2/node_modules/sparkline/bin/sparkline" "$@"
else
  exec node  "$basedir/../../../../../sparkline@0.1.2/node_modules/sparkline/bin/sparkline" "$@"
fi
