#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/projects/ke/code/test-maker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/projects/ke/code/test-maker/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../json2csv@5.0.7/node_modules/json2csv/bin/json2csv.js" "$@"
else
  exec node  "$basedir/../../../../../json2csv@5.0.7/node_modules/json2csv/bin/json2csv.js" "$@"
fi
