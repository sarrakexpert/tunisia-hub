{
  "version": "1.3.7",
  "name": "pega-model",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "es2015": "src/index.ts",
  "scripts": {
    "dev": "webpack --watch --config webpack.config.js --mode=development",
    "prod": "webpack --config webpack.config.js --mode=production",
    "publish": "npm publish  --registry http://116.203.54.179:4873",
    "test:local": "ketm run --config test-maker.local.ts",
    "test:local:eye": "ketm run --config test-maker.local.ts --terminal-eye",
    "test:ci": "ketm run --config test-maker.ci.ts",
    "test:filter:scenario": "npx ketm run --config test-maker.local.ts --filter-scenario \"E2E\"\" test case\"",
    "test:filter": "ketm run --config test-maker.local.ts --filter.glob \"./src/specs/custom-control-spec.ts\"",
    "test:filter:tag": "ketm run --config test-maker.local.ts --filter-feature-tag \"smoke\"",
    "test:filter:scenario:tag": "ketm run --config test-maker.local.ts --filter-scenario-tag \"scenario || scenario3\"",
    "compile": "ketm run --config test-maker.ci.ts -- --override {build: {compiler: {compileOnly:true}}}"
  },
  "author": "Knowledge Expert",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "p-queue": "^6.4.0",
    "test-maker": "test-maker-7.5.0-rc.3.tar.gz",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.0-3"
  }
}
